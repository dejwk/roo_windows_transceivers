{
    "name": "roo_windows_transceivers",
    "description": "GUI OneWire thermometers configurator for ESP32, using roo_windows.",
    "keywords": [
        "roo_windows",
        "OneWire"
    ],
    "authors": [
        {
            "name": "Dejwk",
            "email": "dejwk.oss@gmail.com",
            "maintainer": true
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/dejwk/roo_windows_transceivers"
    },
    "version": "1.0.0",
    "homepage": "https://github.com/dejwk/roo_windows_transceivers",
    "frameworks": [
        "Arduino"
    ],
    "build": {
        "srcDir": "src",
        "srcFilter": "+<*> -<examples> -<benchmarks> -<doc> -<test>"
    },
    "examples": [
        "examples/**/*.ino"
    ],
    "dependencies": {
        "dejwk/roo_control": ">=1.1.1",
        "dejwk/roo_onewire": ">0",
        "dejwk/roo_windows": ">=1.3.0"
    }
}
